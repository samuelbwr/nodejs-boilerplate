{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    ".editorconfig"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "test/",
    "config/",
    "controllers/",
    "models/",
    "helpers/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json hbs html"
}
